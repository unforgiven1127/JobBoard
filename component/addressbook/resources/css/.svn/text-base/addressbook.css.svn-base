  min-width: 32px;
  max-width: 60px;
  text-align: center;
  float: left;
  cursor: pointer;
}
.searchMenuIcon span
{
  color: #fff;
  font-size: 11px;
}

.searchContainer
{
  position: relative;
  float: left;

  /*margin: -250px 0 0 23px;*/
  margin: -150px 0 0 3px;
  padding: 0;
  z-index: 50000;

  /* #45A6D3  */
  border-top: 4px solid #444;
  border-right: 4px solid #444;
  border-bottom: 4px solid #444;

  background-color: #ececec;
  margin-bottom: 30px;

  -webkit-box-shadow: 0px 0px 100px #888;
  -moz-box-shadow: 0px 0px 100px #888;
  box-shadow: 0px 0px 100px #888;

  -moz-border-radius-topright: 10px;
  -webkit-border-top-right-radius: 10px;
  border-top-right-radius: 10px;

  -moz-border-radius-bottomright: 10px;
  -webkit-border-bottom-right-radius: 10px;
  border-bottom-right-radius: 10px;

}
.queryBuilderContainer
{
  width: 1050px;
  padding: 10px 0;
}

.searchFormContainer
{
  float: left;
  width: 75%;
  min-width: 550px;
  min-height: 150px;
}

.searchFormSidebar
{
  float:left;
  width: 160px;
  min-height: 150px;
  border-right: 2px solid #fff;
  margin-left: 15px;
}
.searchFormSidebar ul li
{
  float: left;
  width: 130px;
  border: 1px solid #45A6D3;
  margin: 2px auto;
  padding: 1px 0 0 10px;
  color: #fff;
  cursor: pointer;

  background-color: #DEE8ED;
  -webkit-border-radius: 6px ;
  -moz-border-radius: 6px;
  border-radius: 6px;
}
.searchFormSidebar ul li.fieldUsed
{
  border: 1px solid #fff;
}
.searchFormSidebar ul li.fieldUsed a
{
  font-style: italic;
  color: #999;
}

.searchFormContainer form,
.searchFormContainer form .innerForm,
.searchFormContainer form .innerForm td
{
  border: 0;
  margin-left: 0;
  margin-right: 0;
  padding-left: 0;
  padding-right: 0;
}

.searchFormHeader
{
  width: 84%;
  border-bottom: 3px solid #FFDFB2;
}
.searchTitle
{
  padding: 5px 15px 5px;
  color: #245C91;
  font-size: 15px;
  font-weight: bold;

}
.searchOption
{
  position: absolute;
  right: 8px;
  top: 0;
}

.searchOption div
{
  height: 24px;
  background-color: #444;
}
.searchOption div a
{
  color: #fff;
}
.searchOption div.searchOptionClear
{
  float: left;
  border-right: 1px dashed #eee;
  padding: 2px 10px 2px 10px;
  background-color: transparent;
}
.searchOption div.searchOptionClose
{
  background: #444 url('/component/addressbook/resources/pictures/close_search_form.png') no-repeat right 3px;
  border-left: 1px dashed #eee;
  float: right;
  cursor: pointer;
  padding: 0 25px 0 5px;

  -moz-border-radius-bottomright: 5px;
  -webkit-border-bottom-right-radius: 5px;
  border-bottom-right-radius: 5px;
}
.searchOption div.searchOptionClose div{ margin-top: 2px; height: auto; color: #fff; }

.searchOption div.searchOptionClear a
{
  color: #45A6D3;
  font-weight: bold;
}

.searchOption ul
{
  min-width: 100px;
  float: right;
  background: #444;
  height: 24px;

  -moz-border-radius-bottomleft: 5px;
  -webkit-border-bottom-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
.searchOption ul li
{
  background: #444;
  color: #aaa;
  font-style: italic;
  padding: 4px 5px 4px 5px;
  margin: 0;
  text-align: right;
  cursor: pointer;
}
.searchOption ul li:first-child
{
  display: list-item;
  padding: 2px 25px 2px 0;
  background: url('/component/addressbook/resources/pictures/open_option.png') no-repeat right 3px;
  border: none;
}

.searchTitleSortMsg
{
  font-weight: normal;
  font-style: italic;

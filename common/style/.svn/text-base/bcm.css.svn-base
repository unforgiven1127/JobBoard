{
  display: block;
}

#menuNavContainerId ul li ul
{
  display:none;
  background-color: #333333;
  padding: 8px 3px 3px 3px;
  position: absolute;
  top: 35px;
  left: -16px;
  z-index:1000;

  background-color: #333;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#333), to(#777) );
  background-image: -moz-linear-gradient(top, #333, #777 );

  border-left: 2px solid #45A6D3;
  border-right: 2px solid #45A6D3;
  border-bottom: 2px solid #45A6D3;
  -moz-border-radius-bottomleft:  5px;  /* Firefox up to version 3.6 */
  -webkit-border-bottom-left-radius: 5px;  /* Safari, Chrome (before WebKit version 533) */
  border-bottom-left-radius: 5px;

  -moz-border-radius-bottomright:  5px;  /* Firefox up to version 3.6 */
  -webkit-border-bottom-right-radius: 5px;  /* Safari, Chrome (before WebKit version 533) */
  border-bottom-right-radius: 5px;

}
#menuNavContainerId ul li ul li
{
  display: list-item;
  margin: 5px 15px;
  z-index:1001;
  position:relative;
}

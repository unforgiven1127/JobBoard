@CHARSET "UTF-8";

*
{
  font-family: Verdana, Arial;
  font-size: 12px;
  margin:0px;
  color:#333;
}

a
{
  text-decoration:none;
  color: #2A6991;
}
a:hover
{
  text-decoration:none;
  color: orange;
}

ul
{
  margin: 0;
  padding: 0px;
  display: inline;
}

ul li
{
  margin: 0;
  padding: 0;
  list-style: none;
  display: inline;
  color: #0D79BC;
}

img
{
  border:0;
}

.floatHack,
*.floatHack
{
  float:none;
  clear:both;
}

.alignRight {text-align: right; }
.alignleft  {text-align: left; }
.alignCenter  {text-align: center; }

.hidden
{
  display:none;
}

.clickable
{
  cursor: pointer;
}
a.niceTimeLink
{
  text-decoration: none;
  margin-left: 5px;
  position:relative;
  color:#C18243;
}

a.niceTimePic
{
  padding-right: 24px;
  background: url('/common/pictures/date_16.png') no-repeat top right;
}

a.niceTimeLink div
{
  display: none;
  /*color: #fff;*/
  background-color: #333;
  padding: 3px;
  -moz-border-radius:  3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  white-space: nowrap;
}
a.niceTimeLink:hover div
{
  position:absolute;
  top: 0px;
  left: 0px;
  display: block;

}

/* Make a "div" (span + this css) sticking to its content size */
.fittingBloc
{
  display:-moz-inline-stack;
  display:inline-block;
  zoom:1;
  *display:inline;
}

input[type=text], input[type=radio], input[type=checkbox], input[type=password],
select, textarea
{
  border: 1px solid #999999;
  background-color: #f9f9f9;
}


textarea
{
  min-height: 70px;
  min-width: 300px;
}

select option:nth-child(even)
{
  background-color: #ededed;
}
.optionInactive
{
  font-style: italic;
  color: #666;
}

input[type=button], input[type=submit]
{
  border: 1px solid #666;
  -moz-border-radius:  10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  cursor: pointer;

  background-color: #427B9B;
  font-size: 14px;
  color: #fff;
  margin: 0 2px;
  padding:2px 10px;

  -webkit-box-shadow: 4px 4px 5px #bbb;
  -moz-box-shadow: 4px 4px 5px #bbb;
  box-shadow: 4px 4px 5px #bbb;
}
input[type=button]:hover, input[type=submit]:hover
{
  /*border-color: orange;*/

  border: 1px solid #3398F7;
  background-color: #2C74B7;
  -webkit-box-shadow: 4px 4px 5px #666;
  -moz-box-shadow: 4px 4px 5px #666;
  box-shadow: 4px 4px 5px #666;
}
/* ************************************************************** */
/* ************************************************************** */
/* ************************************************************** */
.title
{
  /*font-weight:bold;  */
  margin: 10px 0;
}
.titleToggle
{
  cursor: pointer;
}

.title div a
{
  margin-bottom:10px;
  color:#245C91;
}

.titleLine
{
  margin:10px;
  background-color: #F4FBFF;
  border: 1px solid #93C0FF;
  border-radius: 5px 5px 5px 5px;
  padding: 5px 10px;
}

.titleLinePicture,
.titleLinePicture img
{
  float:left;
  height: 48px;
  width:48px;
}
.titleLineText
{
  border-left: 1px solid #666666;
  margin: 0 0 0 65px;
  padding-left: 6px;
}
.titleLineText div
{
  margin: auto 0;
  vertical-align: top;
}

.subtitle
{
  color: #245C91;
  font-size: 15px;
  font-weight: bold;
  padding: 5px 0 3px 15px;
}

.h1
{
  font-size: 24px;
  color: #245C91;
  /*border-bottom: 2px solid #2F95FA;*/
  text-transform: uppercase;
  font-weight:bold;
}

.h2
{
  /*border-bottom: 2px solid #2F95FA;*/
  font-size: 16px;
  color: #077AC1;
  text-transform: uppercase;

  background-color: #F4FBFF;
  border: 2px solid #88B1CE;
  padding: 3px 15px;

  -moz-border-radius:  8px;
  -webkit-border-radius: 8px;
  border-radius: 8px;
}

.h3
{
  color:#000;
  font-size: 15px;
  font-weight: bold;
  padding: 0 5px;
}

.h4
{
  /*border-bottom: 1px dotted #ababab;*/

  color:#000;
  font-size: 13px;
  font-weight: bold;
  padding: 0 5px;
}

.h5
{
  color:#555;
  font-size: 11px;
  font-weight: bold;
  padding: 0 5px;
}
.h5 span
{
  background-color:#ffffff;
  margin:0;
  padding-right:10px;

}

.bold,
.strong
{
  font-weight:bold;
}

.light,
.light *
{
  color: #999;
}

.italic, .italic *
{
  font-style: italic;
  font-size: 10px;
}

.shadow
{
  -webkit-box-shadow: 4px 4px 5px #ddd;
  -moz-box-shadow: 4px 4px 5px #ddd;
  box-shadow: 4px 4px 5px #ddd;
}


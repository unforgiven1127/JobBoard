{
  width: 200px;
  float: left;
  margin-right: 10px;
}

.fc-content .fc-agenda-slots td div { height: 20px; }
#eventPopupId
{
  background-color: #444444;
  border: 2px solid #FFFFCC;
  color: #FFFFFF;
  cursor: pointer;
  min-height: 40px;
  max-height: 250px;
  padding: 10px;
  position: absolute;
  width: 250px;
  z-index: 99999;

  overflow-y: auto;

  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  border-radius: 10px;

  webkit-box-shadow: 0px 0px 20px yellow;
  -moz-box-shadow: 0px 0px 20px yellow;
  box-shadow: 0px 0px 20px yellow;
  display: none;
}
#eventPopupId span.eventPopupTitle
{
  font-style: italic;
  color: #1D9B56;
}
#eventPopupId span.eventPopupDesc
{
  font-style: italic;
  color: #7094DB;
}
#eventPopupId div.eventPopupDesc
{
  border-left: 2px solid #c6c6c6;
  padding-left: 3px;
  margin: 2px 0 0 5px;
  color: #fff;
}
#eventPopupId span.eventPopupAttend
{
  font-style: italic;
  color: #E2932B;
}
#eventPopupId div.eventPopupAttend
{
  color: #fff;
}
.eventClickable
{
  cursor: pointer;
}
.fc-event
{
  min-height: 6px;
}
  font-size:14px;
  background-color: #333;
  height:38px;
}

#menuNavContainerId
{
  background-color:#ededed;
  height: 18px;
  margin-top: 30px;
  width:980px;
  margin: 0 auto;
  z-index:99999;
}

#menuNavContainerId ul { height: 15px; float: left; width: 100%; }
#menuNavContainerId ul li { float: left; }

.menuNavList
{
  float: left;
  height: 32px;
  width: 100%;
  padding-left: 240px;
  z-index:100;
}
#menuNavContainerId ul li
{
  margin:0 5px;
  position:relative;
  z-index:101;
}
#menuNavContainerId ul li a,
#menuNavContainerId ul li a img
{
  position:relative;
  z-index:102;
}
#menuNavContainerId ul li:hover a img
{
  top: -12px;
}


#menuNavContainerId ul li:hover
{
    z-index:501;
}
#menuNavContainerId ul li:hover ul
{
  display: block;
}

#menuNavContainerId ul li ul
{
  display:none;
  background-color: #333333;
  padding: 8px 3px 3px 3px;
  position: absolute;
  top: -1px;
  left: -16px;
  z-index:1000;


  border-left: 2px solid #45A6D3;
  border-right: 2px solid #45A6D3;
  border-bottom: 2px solid #45A6D3;
  -moz-border-radius-bottomleft:  5px;  /* Firefox up to version 3.6 */
  -webkit-border-bottom-left-radius: 5px;  /* Safari, Chrome (before WebKit version 533) */
  border-bottom-left-radius: 5px;

  -moz-border-radius-bottomright:  5px;  /* Firefox up to version 3.6 */
  -webkit-border-bottom-right-radius: 5px;  /* Safari, Chrome (before WebKit version 533) */
  border-bottom-right-radius: 5px;

}
#menuNavContainerId ul li ul li
{
  display: list-item;
  margin: 5px 15px;
  z-index:1001;
  position:relative;
}
#menuNavContainerId ul li ul li a
{
  font-size:1.2em;
  color:#ffffff;
  white-space: nowrap;
}

#menuNavContainerId ul li ul li:hover,
#menuNavContainerId ul li ul li a:hover
{
  color:#45A6D3;
}



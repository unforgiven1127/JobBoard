#calendarViewFormId,
#calendarViewFormId div.innerForm,
#calendarViewFormId div.innerForm .formFieldContainer
{
  margin: 0;
  padding: 0;
  background-color: transparent;
  border: 0;
  border-radius: 0;
  box-shadow: 0;
  min-height: 0;
  height: auto;
}

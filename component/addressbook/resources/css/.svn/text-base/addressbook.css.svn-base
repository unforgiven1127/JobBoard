/* name(s) bloc in the cp_card view */
.cp_top_name,
.ct_top_name_male,
.ct_top_name_female
{
   padding: 0 20px 4px 5px;
  margin: 0 0 5px 0;
  border-bottom: 1px solid #cecece;
  text-align: right;
  min-height: 30px;

  -moz-border-radius-bottomleft:  10px;
  -webkit-border-bottom-left-radius: 10px;
  border-bottom-left-radius: 10px;

  -moz-border-radius-topright:  30px;
  -webkit-border-top-right-radius: 30px;
  border-top-right-radius: 30px;
}
.ct_top_name_male div.title
{
  margin: 3px 0;
}

.cp_top_name
{
  background-color: #FFF9EF;
  background-image: -webkit-gradient(linear, right top, left top, from(#FFF3E0), to(#FFF9EF) );
  background-image: -moz-linear-gradient(right, #FFF3E0, #FFF9EF );
}
.ct_top_name_male
{
  background-color: #F1FAFF;
  background-image: -webkit-gradient(linear, right top, left top, from(#F1FAFF), to(#F8FDFF) );
  background-image: -moz-linear-gradient(right, #F1FAFF, #F8FDFF );
}
.ct_top_name_female
{
  background-color: #FCE7E7;
  background-image: -webkit-gradient(linear, right top, left top, from(#FCE7E7), to(#FDF7F6) );
  background-image: -moz-linear-gradient(right, #FCE7E7, #FDF7F6 );
}

.ct_top_name_female div.title div.h4 a
{
  color:#555;
}
.ct_top_name_female div.title div.h4 a:hover
{
  color:#418FB3;
}
.ct_top_name_male div.title div.h4 a
{
  color:#555;
}
.ct_top_name_male div.title div.h4 a:hover
{
  color:#418FB3;
}

.cp_top_contact
{
  margin: 5px 0 0 0;
  width: 100%;
  float:left;
  font-size: 11px;
}

.cp_top_activity
{
  margin:0px 0 5px 15px;
  padding:10px 65px 0 10px;
  float:left;
  min-height:72px; /* size of the bg picture */
  background: url("/component/addressbook/resources/pictures/cp_activity_bg_72.png") no-repeat right center;

  border: 1px solid #EAF4FF;
 -webkit-border-radius: 10px ;
 -moz-border-radius: 10px;
  border-radius: 10px;
  width: 48%;
}

.cp_top_action
{
 min-width: 48%;
 margin:7px 0 0 15px;
 padding:7px 65px 10px 10px;
 float:left;
 min-height: 30px;
 background: url("/component/addressbook/resources/pictures/cp_action_bg_64.png") no-repeat right center;

 border: 1px solid #EAF4FF;
-webkit-border-radius: 10px ;
-moz-border-radius: 10px;
 border-radius: 10px;
}
.cp_top_activity a,
.cp_top_action a,
.cp_top_contact a,

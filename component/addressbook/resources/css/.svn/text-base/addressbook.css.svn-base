.cp_top_container,
.ct_top_container
{
  border: 1px solid #cecece;
  margin:15px;
  padding:10px;
  float:left;
  height: 230px;
  width: 440px;

  -webkit-border-radius: 10px ;
  -moz-border-radius: 10px;
  border-radius: 10px;
}

.cp_top_container
{
  background: url("/component/addressbook/resources/pictures/cp_card_bg_128.png") no-repeat bottom right;
}
.ct_top_container_male
{
    background: url("/component/addressbook/resources/pictures/ct_card_bg_128.png") no-repeat bottom right;
}
.ct_top_container_female
{
    background: url("/component/addressbook/resources/pictures/ct_card_bg_woman_128.png") no-repeat bottom right;
}
.ct_top_container_female,
.ct_top_container_male
{
  border: 1px solid #cecece;
  margin:15px;
  padding: 5px 10px;
  float:left;
  height: 230px;
  width: 440px;

  -webkit-border-radius: 10px ;
  -moz-border-radius: 10px;
  border-radius: 10px;
}

